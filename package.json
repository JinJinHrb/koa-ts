{
  "name": "koa-ts",
  "version": "3.2.1",
  "license": "MIT",
  "description": "The best practice of building Koa2 with TypeScript",
  "main": "app.ts",
  "scripts": {
    "start": "export NODE_ENV=development; ts-node -r tsconfig-paths/register app.ts",
    "dev": "export NODE_ENV=development; ts-node-dev --transpile-only -O '{\"esModuleInterop\": true}' --respawn --inspect -r tsconfig-paths/register app.ts",
    "dev:db": "docker compose -f docker-compose.yml up -d",
    "prettier": "prettier --write '**/*.{js,ts}'",
    "test": "jest --config .jest.config.js --no-cache --detectOpenHandles",
    "prod:build": "node ./build.js",
    "prod:start": "prisma generate && prisma migrate deploy && export NODE_ENV=production; node ./dist/index.js",
    "lint": "eslint app/**/*.ts",
    "format": "eslint app/**/*.ts --fix"
  },
  "author": "unix (unix.bio@gmail.com)",
  "bugs": {
    "url": "https://github.com/unix/koa-ts/issues"
  },
  "repository": "git@github.com:unix/koa-ts.git",
  "engines": {
    "node": ">= 14.x"
  },
  "prettier": "@geist-ui/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "devDependencies": {
    "@geist-ui/prettier-config": "^1.0.1",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^25.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.5",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-mount": "^4.0.5",
    "@types/koa-static": "^4.0.4",
    "@types/lodash": "^4.14.186",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "esbuild": "^0.14.11",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^8.24.0",
    "jest": "^26.6.3",
    "prettier": "^2.7.1",
    "prisma": "^3.8.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@babel/core": "^7.21.4",
    "@babel/generator": "^7.21.4",
    "@babel/parser": "^7.21.4",
    "@babel/traverse": "^7.21.4",
    "@prisma/client": "^3.8.1",
    "axios": "^1.6.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.7",
    "dompurify": "^3.0.6",
    "dotenv": "^12.0.3",
    "exceljs": "^4.4.0",
    "fs": "^0.0.1-security",
    "generic-pool": "^3.9.0",
    "graphology": "^0.25.1",
    "graphology-types": "^0.24.7",
    "husky": "^8.0.1",
    "is-image": "^3.1.0",
    "jsdom": "^23.0.1",
    "json-object-mapper": "^1.7.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-multer": "^1.0.2",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "lru_map": "^0.4.1",
    "mime-types": "^2.1.35",
    "puppeteer": "^22.8.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "typedi": "^0.10.0",
    "wildcard": "^2.0.0"
  }
}
